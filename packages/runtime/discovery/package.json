{
  "name": "@stackpro/discovery",
  "version": "0.1.0",
  "description": "StackPro service discovery runtime",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "service-discovery",
    "aws",
    "ssm"
  ],
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.863.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  }
}
