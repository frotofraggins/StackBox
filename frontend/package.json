{
  "name": "stackbox-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "next start -p 3000",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "sandbox": "npx ampx sandbox",
    "deploy": "npx ampx pipeline deploy --branch main",
    "snap:before": "npm run build && npm run preview & sleep 3 && node ../tools/visual-diff/capture.js before",
    "snap:after": "npm run build && npm run preview & sleep 3 && node ../tools/visual-diff/capture.js after",
    "snap:index": "node ../tools/visual-diff/index.js",
    "health": "node -e \"fetch('http://localhost:3000/health').then(r=>r.json()).then(console.log)\""
  },
  "dependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@aws-amplify/ui-react": "^6.11.2",
    "@fontsource/inter": "^5.2.6",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "aws-amplify": "^6.15.5",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4"
  }
}
