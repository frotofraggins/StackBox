# StackPro Environment Configuration
# Copy to .env.local and customize

# API Configuration
NEXT_PUBLIC_API_URL=/api
NEXT_PUBLIC_WS_URL=wss://ws-sandbox.stackpro.io

# Environment
NEXT_PUBLIC_ENV=sandbox
NEXT_PUBLIC_FREE_TIER=true

# Features
AI_ENABLED=false

# Data Platform Feature Flags (all OFF by default)
MARKETPLACE_ENABLED=false
ETL_ENABLED=false
IOT_ENABLED=false
DATA_SELLER_PORTAL_ENABLED=false

# Data Platform Configuration
ANON_POLICY_ID=default-v2
K_ANON_K=20
S3_LAKE_RAW=stackpro-lake-raw
S3_LAKE_CURATED=stackpro-lake-curated
