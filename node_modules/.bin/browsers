#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/browsers/lib/cjs/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/browsers/lib/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/browsers/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/node_modules:/mnt/d/Users/nflos/StackBox/node_modules:/mnt/d/Users/nflos/node_modules:/mnt/d/Users/node_modules:/mnt/d/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/browsers/lib/cjs/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/browsers/lib/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/browsers/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/@puppeteer/node_modules:/mnt/d/Users/nflos/StackBox/node_modules:/mnt/d/Users/nflos/node_modules:/mnt/d/Users/node_modules:/mnt/d/node_modules:/mnt/d/Users/nflos/StackBox/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@puppeteer/browsers/lib/cjs/main-cli.js" "$@"
else
  exec node  "$basedir/../@puppeteer/browsers/lib/cjs/main-cli.js" "$@"
fi
